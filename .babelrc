{
    "plugins": [
        [
            "module-resolver",
            {
                "cwd": "babelrc",                
                "alias": {
                    "Constants": "./app/src/constants",
                    "Components": "./app/src/components",
                    "Images": "./resources/images"
                }
            },
            "@babel/plugin-syntax-dynamic-import"
        ]
    ],
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ]
}